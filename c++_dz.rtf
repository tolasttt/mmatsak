class MinHeap \{\
    // \uc0\u1056 \u1077 \u1072 \u1083 \u1080 \u1079 \u1072 \u1094 \u1080 \u1103  \u1084 \u1080 \u1085 -\u1082 \u1091 \u1095 \u1080  (\u1073 \u1080 \u1085 \u1072 \u1088 \u1085 \u1072 \u1103  \u1082 \u1091 \u1095 \u1072 )\
    std::vector<int> a;\
    void sift_up(int i) \{                        // \uc0\u1087 \u1086 \u1076 \u1098 \u1105 \u1084 \
        while (i > 0) \{\
            int p = (i - 1) / 2;\
            if (a[p] <= a[i]) break;\
            std::swap(a[p], a[i]);\
            i = p;\
        \}\
    \}\
    void sift_down(int i) \{                      // \uc0\u1086 \u1087 \u1091 \u1089 \u1082 \u1072 \u1085 \u1080 \u1077 \
        int n = (int)a.size();\
        while (true) \{\
            int l = 2*i + 1, r = 2*i + 2, s = i;\
            if (l < n && a[l] < a[s]) s = l;\
            if (r < n && a[r] < a[s]) s = r;\
            if (s == i) break;\
            std::swap(a[i], a[s]);\
            i = s;\
        \}\
    \}\
public:\
    void push(int x) \{ a.push_back(x); sift_up((int)a.size() - 1); \} // \uc0\u1074 \u1089 \u1090 \u1072 \u1074 \u1082 \u1072 \
    int top() const \{ return a[0]; \}                                 // \uc0\u1084 \u1080 \u1085 \u1080 \u1084 \u1091 \u1084 \
    void pop() \{                                                     // \uc0\u1091 \u1076 \u1072 \u1083 \u1080 \u1090 \u1100  \u1084 \u1080 \u1085 \u1080 \u1084 \u1091 \u1084 \
        std::swap(a[0], a.back());\
        a.pop_back();\
        if (!a.empty()) sift_down(0);\
    \}\
    bool empty() const \{ return a.empty(); \}\
\};\
