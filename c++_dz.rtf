template <typename T>\
class Stack \{\
    // \uc0\u1056 \u1077 \u1072 \u1083 \u1080 \u1079 \u1072 \u1094 \u1080 \u1103  \u1089 \u1090 \u1077 \u1082 \u1072  \u1085 \u1072  \u1074 \u1077 \u1082 \u1090 \u1086 \u1088 \u1077 \
    std::vector<T> a;\
public:\
    void push(const T& x) \{ a.push_back(x); \}   // push\
    void pop() \{ a.pop_back(); \}                // pop\
    T& top() \{ return a.back(); \}               // peek\
    bool empty() const \{ return a.empty(); \}\
    size_t size() const \{ return a.size(); \}\
\};\
